{
  "private": true,
  "name": "api-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "cross-env NODE_ENV=production node dist/main/server.js",
    "test": "jest --passWithNoTests",
    "test:cov": "yarn test --coverage",
    "dev": "tsnd -r tsconfig-paths/register --inspect --exit-child --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "cross-env DATABASE_PORT=5432 && yarn dev",
    "prisma:dev": "cross-env DATABASE_HOST=localhost prisma",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js)\" --write"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "schema": "src/infra/database/prisma/schema.prisma"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/node": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@swc/core": "1.3.28",
    "@swc/jest": "0.2.24",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.16",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "git-commit-msg-linter": "4.5.0",
    "jest": "29.4.0",
    "prettier": "2.8.3",
    "prisma": "4.9.0",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "rimraf": "4.1.1",
    "swagger-ui-express": "4.6.0"
  }
}
