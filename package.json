{
  "private": true,
  "name": "api-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --inspect --exit-child --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "cross-env DATABASE_PORT=5432 && yarn dev",
    "prisma:dev": "cross-env DATABASE_HOST=localhost prisma"
  },
  "engines": {
    "node": "14.19.3"
  },
  "prisma": {
    "schema": "src/infra/database/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "18.0.0",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "cross-env": "7.0.3",
    "eslint": "8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "git-commit-msg-linter": "4.1.3",
    "prettier": "2.7.1",
    "prisma": "3.15.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@prisma/client": "3.15.2",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "helmet": "5.1.0",
    "swagger-ui-express": "4.4.0"
  }
}
